import numpy as np

# Define the vector v
v = np.array([2., 2., 4.])

# Define the unit vectors for the 3D coordinate axes
e0 = np.array([1, 0, 0])
e1 = np.array([0, 1, 0])
e2 = np.array([0, 0, 1])

# Calculate the projections
proj_e0 = np.dot(v, e0)
proj_e1 = np.dot(v, e1)
proj_e2 = np.dot(v, e2)

# Print the results
print(f"Projection onto e0: {proj_e0}")
print(f"Projection onto e1: {proj_e1}")
print(f"Projection onto e2: {proj_e2}")